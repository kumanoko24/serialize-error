	"dependencies": {
		"type-fest": "^2.12.2"
	},
	"devDependencies": {
		"ava": "^4.2.0",
		"esbuild": "^0.14.38",
		"tsd": "^0.20.0",
		"xo": "^0.48.0"
	}
